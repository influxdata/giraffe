    "start": "webpack --config webpack.dev.config.js --watch --progress",
    "test": "jest --collectCoverage --maxWorkers=2",
    "test:watch": "jest --collectCoverage --watch --verbose false",
    "typecheck": "tsc --noEmit --pretty"
  },
