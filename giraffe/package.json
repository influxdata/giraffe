  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack --config webpack.prod.config.js",
    "lint": "eslint '{src,../stories/src}/**/*.{ts,tsx}'",
    "prettier": "pretty-quick --config ../.prettierrc.json --check '{src,../stories/src}/**/*.{ts,tsx}'",
    "prettier:fix": "pretty-quick --config ../.prettierrc.json --write '{src,../stories/src}/**/*.{ts,tsx}'",
